{
  "name": "gatsby-source-atom",
  "description": "Gatsby source plugin for adding an Atom feed as a data source",
  "version": "0.3.0",
  "author": "Pawan Hegde <contact@pawanhegde.com>",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "main": "index.js",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "atom"
  ],
  "license": "MIT",
  "repository": "https://github.com/PawanHegde/gatsby-source-atom",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "feedparser": "^2.2.9",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "cross-env": "^7.0.2"
  },
  "peerDependencies": {
    "gatsby": "^2.20.12"
  },
  "renovate": {
    "extends": [
      "config:base",
      ":preserveSemverRanges"
    ],
    "automerge": true
  }
}
